{
  "name": "openapi-redux-sdk-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for OpenAPI Redux SDK Generator",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "busboy": "^1.6.0",
    "archiver": "^6.0.1",
    "@apidevtools/swagger-parser": "^10.1.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/busboy": "^1.5.3",
    "@types/archiver": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "firebase-functions-test": "^3.1.0",
    "openapi-types": "^12.1.3"
  },
  "private": true
}
